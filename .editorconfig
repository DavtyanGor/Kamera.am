# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*.{css,scss,sass,less.js,jsx,.module.scss,.module.sass,.module.less,.module.css}]
indent_style = tab
indent_size = 2
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false